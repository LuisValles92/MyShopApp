<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorNegro"
    tools:context=".ClienteActivity.ProductSearchActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar10"
        style="@style/Toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Buscador de productos"
        app:titleTextColor="@color/colorBlanco" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler2Productos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="56dp"
        android:theme="@style/RecyclerTheme"
        app:layout_constraintTop_toBottomOf="@+id/toolbar0"
        tools:listitem="@layout/item_productosearch" />
</androidx.constraintlayout.widget.ConstraintLayout>